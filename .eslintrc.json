{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "@typescript-eslint"
  ],
  "rules": {
    "no-trailing-spaces": "error",
    "no-multiple-empty-lines": "error",
    "no-unused-vars": "off",
    "no-undef": "off"
  }
}
