{
  "source": ["properties/**/*.json"],
  "platforms": {
    "scss": {
      "transformGroup": "scss",
      "buildPath": "src/scss/variables/",
      "prefix": "tec",
      "files": [
        {
          "destination": "_variables.scss",
          "format": "scss/variables"
        },
        {
          "destination": "_variables.scss",
          "format": "scss/map-flat",
          "mapName": "my-tokens"
        }
      ]
    }
  }
}
